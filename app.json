{
    "name": "course-expo-app",
    "slug": "course-expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#16171d",
    "newArchEnabled": true,
    "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#16171d"
    },
    "ios": {
        "supportsTablet": true
    },
    "android": {
        "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#16171d"
        }
    },
    "web": {
        "favicon": "./assets/favicon.png",
        "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "scheme": "native-expo-scheme"
}
